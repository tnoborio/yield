{
  "private": true,
  "scripts": {
    "css:build": "tailwindcss --input stylesheets/input.css --output static/css/output.css",
    "css:watch": "tailwindcss --input stylesheets/input.css --output static/css/output.css --watch",
    "css:minify": "tailwindcss --input stylesheets/input.css --output static/css/output.css --minify",
    "copy:css": "cp node_modules/@xyflow/react/dist/style.css static/css/reactflow.css",
    "postinstall": "npm run copy:css",
    "build": "npm run css:minify"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.2.1"
  },
  "dependencies": {
    "@xyflow/react": "^12.10.1",
    "process": "^0.11.10",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
